<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.alearner.mapper.mysql.UUserNotifyMapper"><!--注意这里的地址-->
    <select id="getUUserNotifyById"  parameterType="int"  resultType="com.alearner.entity.mysql.UUserNotify">
        select * from u_user_notify where id = #{id} and able_status=true ;
    </select>
    <insert id="addUUserNotify" useGeneratedKeys="true" keyProperty="id" parameterType="com.alearner.entity.mysql.UUserNotify">
        insert into u_user_notify(read_status,user_id,notify_id)values(#{readStatus},#{userId},#{notifyId})
    </insert>

</mapper>